<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.classcon.classcon">

    <application
        android:allowBackup="true"
        android:configChanges="orientation|screenSize"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".LoginActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!--
 ATTENTION: This was auto-generated to add Google Play services to your project for
     App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information.
        -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <activity
            android:name=".MenuPrActivity"
            android:label="@string/title_activity_menu_pr"
            android:parentActivityName=".LoginActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.classcon.classcon.LoginActivity" />

            <category android:name="android.intent.category.LAUNCHER" />
        </activity>
        <activity
            android:name=".menuSecciones"
            android:label="@string/title_activity_menu_secciones"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".CuentaActivity"
            android:label="@string/title_activity_cuenta"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".RegistroActivity" />
        <activity android:name=".ActivityConfigurarSeccion" />
        <activity android:name=".TemarioActivity"></activity>
    </application>

</manifest>